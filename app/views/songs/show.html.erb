<p id="notice"><%= notice %></p>

<h1>
  <%= @song.title %> <br />
  by <%= @song.artist %>
</h1>

<p>
  <%= raw @song.embed_code %>
</p>


<p>
<%= link_to "via: #{@song.provider }", @song.url %>
</p>

<% if can? :manage, @song %>
<%= link_to 'Edit', edit_song_path(@song) %> |
<%= link_to 'Back', songs_path %>
<% end %>